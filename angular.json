{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "container": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/container",
      "sourceRoot": "projects/container/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/container",
            "index": "projects/container/src/index.html",
            "browser": "projects/container/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/container/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/container/public"
              }
            ],
            "styles": [
              "projects/container/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "container:build:production"
            },
            "development": {
              "buildTarget": "container:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/container/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/container/public"
              }
            ],
            "styles": [
              "projects/container/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
